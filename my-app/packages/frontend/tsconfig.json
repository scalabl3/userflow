{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "references": [
    { "path": "../shared" }
  ],
  "include": ["src/**/*", "next-env.d.ts"],
  "exclude": ["node_modules", ".next", "out"]
} 